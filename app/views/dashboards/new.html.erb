 <iframe src="<%= current_user.setup_forms.first.form_url %>" class="full-page"></iframe>

 <script type="text/javascript">


var displayError = function () {
	console.log("There was an error");
};

var checkUrl = function(dashboard) {
	if (dashboard.dashboard_url != null) {
  	window.location = "/dashboards";
  };
};

var request = function() {
  $.ajax({
  url: "/dashboards/created",
  data: "",
  success: checkUrl,
  error: displayError,
  datatype: "json"
	});
}

$(function () {
	setInterval(function(){
		request();
  	}, 1000)
});

</script>
